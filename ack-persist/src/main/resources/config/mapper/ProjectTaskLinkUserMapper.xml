<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.ack.persist.mapper.ProjectTaskLinkUserMapper">
                                                               
     <insert id="insert" parameterType="org.ack.pojo.ProjectTaskLinkUser">
        INSERT INTO
		ack_project_task_user(projectTaskId,userId)
		VALUES
		(#{projectTaskId}, #{userId})
     </insert>
     
     <delete id="deleteByProjectTaskId" parameterType="long">
        DELETE
		FROM
			ack_project_task_user
		WHERE
			projectTaskId = #{projectTaskId}
     </delete>
</mapper>